<div class="landing">
  <section class="section-1">
    <%= if @show_login do %>
      <div
        class="home-bloc colored is-left"
        id="landing-login"
        phx-hook="login"
        data-validated="<%= @validated %>"
        data-email="<%= @email %>"
        data-password="<%= @password %>">
        <div class="header">
          <h1>Log in</h1>
          <a class="home-link" href="#" phx-click="show_signup">Sign up</a>
        </div>

        <%= f = form_for :account, "#", phx_submit: "login" %>
          <div class="body">
            <div class="input-text">
              <%= label f, :email %>
              <%= email_input f, :email, autofocus: true, required: true, "phx-debounce": "500" %>
              <%= error_tag f, :email %>
            </div>

            <div class="input-text">
              <%= label f, :password %>
              <%= password_input f, :password, required: true %>
              <%= error_tag f, :password %>
            </div>
          </div>

          <div class="footer">
            <%= submit "Log in", class: "button" %>
          </div>
        </form>
      </div>
    <% else %>
      <div class="home-bloc colored is-left">
        <div class="header">
          <h1>Create an account for free</h1>
          <a class="home-link" href="#" phx-click="show_login">Log in</a>
        </div>

        <form
          id="signup-form"
          method="post"
          phx-hook="signup">
          <div class="body">
            <div class="input-text">
              <label for="email">Your email address</label>
              <input id="email" type="text" name="email">
            </div>

            <div class="input-text">
              <label for="name">Your pseudo</label>
              <input id="name" type="text" name="name">
            </div>

            <div class="input-text">
              <label for="password1">Choose a password</label>
              <input id="password1" type="password" name="password1">
            </div>

            <div class="input-text">
              <label for="password2">Confirm the password</label>
              <input id="password2" type="password" name="password2">
            </div>
          </div>

          <div class="footer">
            <%= link "I accept the terms and conditions",
              to: Routes.live_path(@socket, Portal.CGULive),
              class: "home-link",
              target: "_blank",
              "phx-click": "show_signup" %>

            <button
              type="submit"
              class="button"
              id="button">
              Create an account
            </button>
          </div>
        </form>
      </div>
    <% end %>

    <div
      id="info-container"
      class="home-bloc is-left"
      style="display: none;">
      <div class="body" id="info"></div>
    </div>

    <img class="landing-image" src="<%= Routes.static_path(@socket, "/img/landing-ships.png") %>">
    <img class="landing-image" src="<%= Routes.static_path(@socket, "/img/landing-ships.gif") %>">
    <img class="landing-image" src="<%= Routes.static_path(@socket, "/img/landing-characters.png") %>">
  </section>

